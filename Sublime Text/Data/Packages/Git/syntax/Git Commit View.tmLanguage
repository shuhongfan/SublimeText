				<key>3</key>
				<dict>
					<key>name</key>
					<string>punctuation.definition.range.diff</string>
				</dict>
			</dict>
			<key>match</key>
			<string>^(@@)\s*(.+?)\s*(@@)($\n?)?</string>
			<key>name</key>
			<string>meta.diff.range.unified</string>
		</dict>
		<dict>
			<key>captures</key>
			<dict>
				<key>3</key>
				<dict>
					<key>name</key>
